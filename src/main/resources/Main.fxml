<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="750.0" prefWidth="1150.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="uni.todo.Controller">
    <VBox prefHeight="750.0" prefWidth="100.0">
        <Button fx:id="buttonAdd" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="350.0"
                prefWidth="100.0" style="-fx-background-color: #263238; -fx-border-color: #ffff5a; -fx-border-width: 3;"
                textAlignment="CENTER">
            <graphic>
                <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@Add.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="scrollFwdButton" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="37.0"
                prefWidth="122.0"
                style="-fx-background-color: #263238; -fx-border-color: #ffff5a; -fx-border-width: 3;">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" translateX="1.0">
                    <Image url="@ArrowRight.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="scrollBwdButton" mnemonicParsing="false" prefHeight="41.0" prefWidth="122.0"
                style="-fx-background-color: #263238; -fx-border-color: #ffff5a; -fx-border-width: 3;">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@ArrowLeft.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="buttonReload" mnemonicParsing="false" prefHeight="350.0" prefWidth="100.0"
                style="-fx-background-color: #263238; -fx-border-color: #ffff5a; -fx-border-width: 3;">
            <graphic>
                <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@Delete.png"/>
                </ImageView>
            </graphic>
        </Button>
    </VBox>
    <VBox prefHeight="700.0" prefWidth="50.0" style="-fx-background-color: #ffff5a;"/>
    <VBox prefHeight="750.0" prefWidth="200.0" style="-fx-border-color: #ffff5a; -fx-border-width: 3;">
        <Label fx:id="mondayLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="200.0"
               style="-fx-background-color: #ffff5a;" text="Montag" textAlignment="CENTER">
            <font>
                <Font size="17.0"/>
            </font>
        </Label>
        <VBox fx:id="mondayBox" prefHeight="720.0" prefWidth="200.0" style="-fx-background-color: #263238;"/>
    </VBox>
    <VBox prefHeight="750.0" prefWidth="200.0" style="-fx-border-color: #ffff5a; -fx-border-width: 3;">
        <Label fx:id="tuesdayLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="200.0"
               style="-fx-background-color: #ffff5a;" text="Dienstag" textAlignment="CENTER">
            <font>
                <Font size="17.0"/>
            </font>
        </Label>
        <VBox fx:id="tuesdayBox" prefHeight="720.0" prefWidth="200.0" style="-fx-background-color: #263238;"/>
    </VBox>
    <VBox prefHeight="750.0" prefWidth="200.0" style="-fx-border-color: #ffff5a; -fx-border-width: 3;">
        <Label fx:id="wednesdayLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="200.0"
               style="-fx-background-color: #ffff5a;" text="Mittwoch" textAlignment="CENTER">
            <font>
                <Font size="17.0"/>
            </font>
        </Label>
        <VBox fx:id="wednesdayBox" prefHeight="720.0" prefWidth="200.0" style="-fx-background-color: #263238;"/>
    </VBox>
    <VBox prefHeight="750.0" prefWidth="200.0" style="-fx-border-color: #ffff5a; -fx-border-width: 3;">
        <Label fx:id="thursdayLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="200.0"
               style="-fx-background-color: #ffff5a;" text="Donnerstag" textAlignment="CENTER">
            <font>
                <Font size="17.0"/>
            </font>
        </Label>
        <VBox fx:id="thursdayBox" prefHeight="720.0" prefWidth="200.0" style="-fx-background-color: #263238;"/>
    </VBox>
    <VBox prefHeight="750.0" prefWidth="200.0" style="-fx-border-color: #ffff5a; -fx-border-width: 3;">
        <Label fx:id="fridayLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="200.0"
               style="-fx-background-color: #ffff5a;" text="Freitag" textAlignment="CENTER">
            <font>
                <Font size="17.0"/>
            </font>
        </Label>
        <VBox fx:id="fridayBox" prefHeight="720.0" prefWidth="200.0" style="-fx-background-color: #263238;"/>
    </VBox>
</HBox>
